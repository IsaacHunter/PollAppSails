    body {
      margin: 0;
    }
    h6, h4 {
        color: #939393;
    }
    h6, .title {
        text-transform: uppercase;
        text-align: center;
    }
    b {
        color: #222;
    }
    a {
        cursor: pointer;
    }
    .row:before, .row:after {
      display:table;
      content:" ";
    }
    .row:after {
      clear: both;
    }
    .burger-buns{
      padding: 80px 0;
    }
    .topper {
        width: 100%;
        padding: 6px 0;
        background: #f0f0f0;
    }
    .gt-logo {
        width: 25px;
        margin-left: auto;
        margin-right: auto;
        display: block;
    }
    .gt-badge {
        margin-top: 22%;
        text-align: center;
    }
    .thumbnail {
        padding: 40px;
        background: #fff;
        border-radius: 5px;
    }
    .ballot .thumbnail {
      border-bottom-left-radius: 0px;
      border-bottom-right-radius: 0px;
    }
    .ballot .button {
      border-top-left-radius: 0px;
      border-top-right-radius: 0px;
      display: block;
    }
    .img-circle {
      border-radius: 500px;
    }
    .portrait {
      height: auto;
      max-width: 175px;
      margin: 0 auto;
      display: block;
    }
    .title {
      font-size: 13px;
      letter-spacing: 2px;
      margin-top: 15px;
    }
    .footer {
      color: #bbbbbb; 
      font-size: 11px; 
      margin-top: 60px;
      letter-spacing: 2px;
      text-transform: uppercase;
      text-align: center;
    }
    .white {
      color: #fff;
    }
    @media (min-width: 768px) {
        .gt-badge {
            padding-left: 15%;
        }
    }
    @media (min-width: 550px) {
        .four.columns {
            width: 33.33333333%;
        }
    }
    
    .sweet-alert button {
      height: auto;
    }
