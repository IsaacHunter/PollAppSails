<div class="container wrap">
  <div class="row">
    <div class="home-header col-xs-12">
      <h1 class="text-center">Welcome!</h1>
			<h4 class="text-center"><%- user.email %></h4>     
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6 col-xs-offset-3">
      <h3 class="">New Question!</h3>
			<form role="form">
			  <div class="form-group">
			    <label for="question-title">Question</label>
			    <input type="email" class="form-control" id="question-title" placeholder="Enter Question">
			  </div>
			  <div class="form-group">
			    <input type="text" class="form-control" name="answer[title]" placeholder="Answer">
			  </div>
			  <button type="submit" class="btn btn-default">Submit</button>
			</form>
    </div>
  </div>
</div>