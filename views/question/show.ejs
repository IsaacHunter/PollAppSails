<div class="container wrap">
  <div class="row">
    <div class="home-header col-xs-12">
      <h1 class="text-center">Results!</h1>
			<h4 class="text-center"><%- question.title %></h4>     
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6 col-xs-offset-3">
      <h3 class="">Answers</h3>
			<ul class="list-group questions">
				<% answers.forEach(function (answer, index) { %>
					<li class="list-group-item">
						<p class="answer-item">
							<%- index + 1 %>: <%- answer.title %> <span>Votes: <%- answer.result %></span>
						</p>
					</li>
				<% }) %>
			</ul>
    </div>
  </div>
</div>